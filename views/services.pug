extends base.pug

block style 
    style
        include ../static/stylecontact.css
block content
    div.container
        h1 book your slot
        form(action="/services" method="post" class="form" ) 
            input(type="text" name="name" placeholder="enter your name" class="myinput")
            input(type="phone" name="phone" placeholder="enter your number" class="myinput")
            input(type="email" name="email" placeholder="enter your mail" class="myinput")
            input(type="text" name="add" placeholder="enter your car number" class="myinput")
            select(name="myselect") 
                option(value='10:00am-11:00am') 10:00am-11:00am
                option(value='11:00am-12:00pm') 11:00am-12:00pm
                option(value='12:00am-1:00pm') 12:00am-1:00pm
                option(value='1:00pm-2:00pm') 1:00pm-2:00pm
                option(value='2:00pm-3:00pm') 2:00pm-3:00pm
                option(value='3:00pm-4:00pm') 3:00pm-4:00pm
                option(value='4:00pm-5:00pm') 4:00pm-5:00pm
                option(value='5:00pm-6:00pm') 5:00pm-6:00pm




                
            button(class='btn')
                | join now